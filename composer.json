{
    "name": "better-serializer/better-serializer-bundle",
    "homepage": "https://github.com/better-serialier/better-serializer-bundle",
    "type": "library",
    "description": "Symfony bundle for the BetterSerializer de/serialization library.",
    "license": "MIT",
    "authors": [
        {
            "name": "Martin Fris / Rastusik",
            "email": "rasta@lj.sk"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "BetterSerializerBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BetterSerializerBundle\\Tests\\": "Tests/"
        }
    },
    "require": {
        "php": ">=7.1",
        "better-serializer/better-serializer": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "squizlabs/php_codesniffer": "^3.0",
        "symfony/var-dumper": "^v3.2",
        "nikic/php-parser": "^3.0",
        "sebastian/phpcpd": "^3.0",
        "friendsofphp/php-cs-fixer": "^2.5",
        "jakub-onderka/php-parallel-lint": "^0.9.2",
        "sensiolabs/security-checker": "^4.0",
        "phpmd/phpmd": "^2.6",
        "phpstan/phpstan": "^0.7",
        "satooshi/php-coveralls": "~1.0",
        "phpro/grumphp": "^v0.12"
    },

    "config": {
        "sort-packages": true
    }
}
